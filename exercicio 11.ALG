Algoritmo "nota"

Var
   n1, n2, n3, media : real

Inicio
      escreval("qual a primeira nota")
      leia(n1)
      escreval("qual a segunda nota")
      leia(n2)
      escreval("qual a terceira nota")
      leia(n3)
      media <- ((n1 * 2 + n2 * 3 + n3 * 5)/10)
      escreval("a media do aluno é", media)

Fimalgoritmo