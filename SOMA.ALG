Algoritmo "soma"

Var
   n1, n2 ,soma, n3 : inteiro
Inicio
      escreval("numero 1")
      leia(n1)
      
      escreval("numero2")
      leia(n2)
      
      escreval("numero3")
      leia(n3)
      
      soma <- (n1 + n2 + n3)
      escreval("seu numero sorteado é", " ", soma)

      se (soma > 0) entao
      escreva("tu e muito bichona")
      fimse



Fimalgoritmo