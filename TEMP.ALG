Algoritmo "temp"

Var
   temp : real


Inicio
      
      escreva("qual a temperatura ")
      leia(temp)
      
      escreval(" entao a temperatura é de"," ", temp)
      
      se temp >= 25 entao
      escreva(" esta muito quente!")
      fimse
      
      se temp < 25 entao
      escreva("  esta agradavel")
      fimse
Fimalgoritmo