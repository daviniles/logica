Algoritmo "nome"

Var
   nome, sobrenome : caracter
Inicio
      escreval("digite seu nome")
      leia(nome)
      
      escreval("seu nome é", nome)
      
      escreval("digite seu sobrenome")
      leia(sobrenome)
      
      escreval("seu sobrenome é", sobrenome)
      
      escreval("obrigado senhor(a)", nome, "", sobrenome)
Fimalgoritmo